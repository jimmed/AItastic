{
  "name": "spankhbot",
  "version": "0.2.0",
  "description": "Twitch bot for great justice",
  "main": "src/main.js",
  "scripts": {
    "start-linux": "./node_modules/electron-prebuilt/dist/electron .",
    "start-osx": "./node_modules/electron-prebuilt/dist/Electron.app/Contents/MacOS/Electron .",
    "watch": "nodemon -x \"node dev-server\" -w webpack.config.development.js",
    "electron-rebuild": "electron-rebuild",
    "dev-linux": "concurrently \"npm run watch\" \"npm run start-linux\"",
    "dev-osx": "concurrently \"npm run watch\" \"npm run start-osx\""
  },
  "dependencies": {
    "electron-prebuilt": "^1.3.2",
    "escape-string-regexp": "^1.0.5",
    "foundation-sites": "^6.2.3",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "moment": "^2.14.1",
    "react": "^15.3.0",
    "react-foundation": "^0.6.8",
    "redux-logger": "^2.6.1",
    "redux-persist": "^3.2.2",
    "redux-persist-transform-immutable": "^3.1.0",
    "redux-thunk": "^2.1.0",
    "slate-irc": "^0.9.0",
    "standard-jsx": "^8.0.0",
    "suitcx": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.5.0",
    "electron-rebuild": "^1.1.5",
    "express": "^4.14.0",
    "less": "^2.7.1",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmed/spankhbot.git"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "keywords": [
    "twitch",
    "electron"
  ],
  "author": "Jim O'Brien (jim@jimmed.net)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/jimmed/spankhbot/issues"
  },
  "homepage": "https://github.com/jimmed/spankhbot#readme"
}
